{
  "private": true,
  "name": "blog",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build && pnpm generate-posts",
    "bundle-analyzer": "ANALYZE=true pnpm run build",
    "clean": "rm -rf .next .turbo",
    "db:check": "drizzle-kit check:mysql",
    "db:generate": "drizzle-kit generate:mysql",
    "db:push": "drizzle-kit push:mysql",
    "db:studio": "drizzle-kit studio",
    "dev": "concurrently \"tsx ./src/lib/auto-refresh.ts\" \"next dev --turbo\"",
    "generate-posts": "tsx ./src/scripts/generate-posts.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^9.3.0",
    "@octokit/rest": "^20.0.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@planetscale/database": "^1.16.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tszhong0411/emails": "workspace:*",
    "@tszhong0411/mdx": "workspace:*",
    "@tszhong0411/ui": "workspace:*",
    "@tszhong0411/utils": "workspace:*",
    "canvas-confetti": "^1.9.2",
    "cobe": "^0.6.3",
    "dayjs": "^1.11.10",
    "drizzle-orm": "^0.29.4",
    "framer-motion": "^11.0.6",
    "geist": "^1.2.2",
    "gray-matter": "^4.0.3",
    "jiti": "^1.21.0",
    "js-sha512": "^0.9.0",
    "lucide-react": "^0.341.0",
    "next": "14.1.1-canary.41",
    "next-auth": "^5.0.0-beta.13",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-medium-image-zoom": "^5.1.10",
    "react-spring": "^9.7.3",
    "resend": "^3.2.0",
    "rss": "^1.2.2",
    "sharp": "^0.33.2",
    "swr": "^2.2.5",
    "use-debounce": "^10.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@tszhong0411/eslint-config": "workspace:*",
    "@tszhong0411/tailwind-config": "workspace:*",
    "@tszhong0411/tsconfig": "workspace:*",
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^20.11.20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "@types/rss": "^0.0.32",
    "@types/ws": "^8.5.10",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.1-canary.41",
    "eslint-plugin-turbo": "^1.12.4",
    "mysql2": "^3.9.2",
    "postcss": "^8.4.35",
    "postcss-lightningcss": "^1.0.0",
    "schema-dts": "^1.1.2",
    "tailwindcss": "^3.4.1",
    "ws": "^8.16.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,md,mdx}": "prettier --cache --write"
  }
