{
  "name": "webpack-multi-entry",
  "version": "1.0.3",
  "description": "webpack5",
  "main": "index.js",
  "scripts": {
    "start": "npm run local --m dev",
    "build": "rimraf dist && npm run pro --m pro",
    "local": "cross-env NODE_ENV=development webpack serve --progress --config build/webpack.dev.conf.js ",
    "pro": "cross-env NODE_ENV=production webpack --progress --config build/webpack.prod.conf.js",
    "server": "live-server ./ --port=8888",
    "analyz": "NODE_ENV=production npm_config_report=true npm run pro"
  },
