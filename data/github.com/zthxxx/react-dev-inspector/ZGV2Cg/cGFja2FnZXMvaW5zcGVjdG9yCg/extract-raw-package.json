    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "storybook": "7.6.6",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.4.2",
    "vitest": "1.1.1"
  }
}
