{
	"name": "tailbreeze",
	"version": "1.0.0",
	"description": "Tailbreeze is a powerful lightweight fully responsive admin template built using HTML5, VanillaJS and TailwindCSS.",
	"scripts": {
		"start": "parcel src/pages/*.html",
		"build": "parcel build src/pages/*.html --no-source-maps --public-url ./"
