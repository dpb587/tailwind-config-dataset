{
  "name": "of",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/jfrazier-eth/of.git",
  "author": "Joe Frazier <54604023+jfrazier-eth@users.noreply.github.com>",
  "license": "MIT",
  "packageManager": "pnpm@8.6.9",
  "engines": {
    "node": "16.18.1"
  },
  "type": "commonjs",
  "scripts": {
    "deploy:server": "gcloud app deploy app.yaml --project=seductive-394121",
    "deploy:queues": "gcloud app deploy app.queues.yaml --project=seductive-394121",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "build": "pnpm clean && tsc -p ./tsconfig.backend.json && pnpm admin:build",
    "clean": "rimraf ./dist",
    "dev": "pnpm build && pnpm migrate up && node -r dotenv/config -r module-alias/register ./dist/apps/backend/index.js",
    "start:server": "cross-env OF_APP=SERVER node -r dotenv/config -r module-alias/register ./dist/apps/backend/index.js",
    "start:queues": "cross-env OF_APP=QUEUES node -r dotenv/config -r module-alias/register ./dist/apps/backend/index.js",
    "test": "node node_modules/jest/bin/jest.js",
    "migrate": "ts-node --project tsconfig.migrations.json node_modules/node-pg-migrate/bin/node-pg-migrate",
    "start:script": "pnpm build && node -r dotenv/config -r module-alias/register ./dist/index.js",
    "admin:build": "pnpm admin:clean && BUILD=admin tsup ./src/apps/admin/index.tsx --inject ./react-import.js && cp ./src/apps/admin/index.html ./public/admin/index.html",
    "admin:dev": "pnpm admin:clean && mkdir ./public/admin && cp ./src/apps/admin/index.html ./public/admin/index.html && BUILD=admin tsup ./src/apps/admin/index.tsx --watch --inject ./react-import.js",
    "admin:clean": "rimraf ./public/admin",
    "ext:dev": "next dev",
    "ext:start": "next start",
    "ext:clean": "rimraf ./ext-dist && rimraf ./only-agent.zip",
    "ext:build": "pnpm ext:clean && concurrently \"pnpm ext:build:popup\" \"pnpm ext:build:background\" \"pnpm ext:build:content\" && pnpm ext:build:firebase && pnpm gulp",
    "ext:build:popup": "next build && concurrently \"pnpm ext:mv:ui:out\" \"pnpm ext:cp:manifest\" && rm -r out",
    "ext:build:background": "BUILD=background tsup ./src/apps/extension/scripts/background.ts",
    "ext:build:content": "BUILD=content tsup ./src/apps/extension/content/content.ts --outDir ./ext-dist --tsconfig ./tsconfig.extension.json && pnpm ext:cp:content:css && pnpm ext:cp:images",
    "ext:build:firebase": "cp -r ./src/apps/extension/firebasejs ./ext-dist/firebasejs",
    "ext:cp:content:css": "cp ./src/apps/extension/content/inject.css ./ext-dist/inject.css",
    "ext:cp:images": "cp ./src/apps/extension/images/* ./ext-dist/",
    "ext:cp:manifest": "cp manifest.json ./ext-dist/manifest.json",
    "p-queue": "tsup ./node_modules/p-queue/dist/index.js --format cjs --outDir ./p-queue",
    "ext:mv:ui:out": "mv out/_next out/next && sed -i '' -e 's/\\/_next/\\.\\/next/g' out/**.html && mv out/index.html ./ext-dist && rsync -va --delete-after out/next/ ./ext-dist/next/",
    "ext:mv:ui:assets": "rsync -va --delete-after public/next-assets ./ext-dist/",
    "zip": "gulp"
  },
  "devDependencies": {
    "@swc/core": "^1.3.68",
    "@tailwindcss/forms": "^0.5.4",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chrome": "^0.0.239",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "28.1.0",
    "@types/node": "^20.3.2",
    "@types/node-cron": "^3.0.8",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/sha1": "^1.1.3",
    "@types/supertest": "^2.0.12",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "jest": "28.1.0",
    "prettier": "^2.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.8",
    "tsup": "^7.1.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@tanstack/react-virtual": "3.0.0-beta.54",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3",
    "nanoid": "3.3.6",
    "neverthrow": "^6.0.0",
    "next": "13.4.10",
    "node-cron": "^3.0.2",
    "p-queue": "6.6.2",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sha1": "^1.1.1",
    "tailwindcss": "3.3.2"
  },
  "optionalDependencies": {
    "bullmq": "^4.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "9.13.0",
    "firebase-admin": "^10.3.0",
    "got": "11.8.3",
    "https-proxy-agent": "^7.0.0",
    "ioredis": "^5.3.2",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.1",
    "pg-promise": "^11.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.9.5"
  },
  "_moduleAliases": {
    "@/backend": "dist/apps/backend",
    "@/sites": "dist/lib/sites",
    "@/utils": "dist/lib/utils",
    "@/extension": "dist/apps/extension",
    "@/lib": "dist/lib"
  },
  "private": true
}
