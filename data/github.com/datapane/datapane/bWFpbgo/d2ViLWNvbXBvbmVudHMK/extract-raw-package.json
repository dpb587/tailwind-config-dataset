        "chroma-js": "^2.4.2",
        "clipboard": "^2.0.10",
        "codemirror": "^5.65.6",
        "debounce": "^1.2.1",
        "downloadjs": "^1.4.7",
        "emitter-component": "^1.1.1",
        "fast-mutex": "^1.0.2",
        "file-saver": "^2.0.5",
