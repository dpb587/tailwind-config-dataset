{
	"name": "svelte-commerce",
	"version": "2.0.1",
	"private": true,
	"scripts": {
		"dev": "node src/lib/services/copy && vite dev --host --port 3000",
		"build": "vite build",
		"start-node": "node build/index.js",
		"start": "NODE_ENV=production && vite preview --host --port 3000",
		"update": "npm exec -- npx ncu -u -p npm i",
		"preview": "vite preview",
		"test": "playwright test --ui",
		"test-local": "playwright test",
		"test-gen": "npx playwright codegen localhost:3000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"partytown": "partytown copylib static/~partytown",
		"gen:manifest": "node ./generate-manifest.js",
		"copy": "node devops/copy",
		"liveS": "node devops/deploy",
		"live": "run-s build copy liveS"
	},
	"dependencies": {
		"@misiki/litekart": "0.0.16",
		"@misiki/litekart-utils": "1.0.13",
		"@vite-pwa/sveltekit": "0.3.0"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.511.0",
		"@aws-sdk/lib-storage": "^3.511.0",
		"@beyonk/svelte-facebook-pixel": "3.0.1",
		"@beyonk/svelte-google-analytics": "2.6.4",
		"@builder.io/partytown": "0.9.1",
		"@egjs/svelte-grid": "1.15.0",
		"@playwright/test": "^1.41.2",
		"@sentry/node": "7.100.1",
		"@sentry/svelte": "7.100.1",
		"@sentry/tracing": "7.100.1",
		"@splidejs/splide-extension-video": "0.8.0",
		"@splidejs/svelte-splide": "0.2.9",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@unpic/svelte": "0.0.51",
		"@woocommerce/woocommerce-rest-api": "1.0.1",
		"amazon-s3-uri": "^0.1.1",
		"autoprefixer": "^10.4.17",
		"cookie": "^0.6.0",
		"cookie-universal": "^2.2.2",
		"cssnano": "^6.0.3",
		"date-fns": "3.3.1",
		"dayjs": "^1.11.10",
		"dotenv": "16.4.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"fuse.js": "7.0.0",
		"hash-it": "^6.0.0",
		"hex-color-to-color-name": "1.0.2",
		"ioredis": "5.3.2",
		"minio": "7.1.3",
		"npm-run-all": "4.1.5",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"shelljs": "0.8.5",
		"ssh2shell": "2.0.9",
		"svelte": "^4.2.10",
		"svelte-confetti": "^1.3.2",
		"svelte-preprocess": "^5.1.3",
		"svelte-share-buttons-component": "2.0.0",
		"svelte-timeago": "0.1.2",
		"svelte-toasts": "^1.1.2",
		"sveltekit-medusa-client": "3.1.2",
		"tailwind-scrollbar": "3.0.5",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "~5.3.3",
		"vanilla-lazyload": "^17.8.8",
		"vite": "^5.1.1",
		"vite-plugin-pwa": "0.17.5",
		"zod": "3.22.4",
		"zod-form-data": "2.0.2"
	},
	"type": "module"
}