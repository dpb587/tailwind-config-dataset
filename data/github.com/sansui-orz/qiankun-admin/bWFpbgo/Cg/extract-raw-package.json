  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd ./mock && npm install && cd ../sub-projects/react-admin && npm install && cd ../vue-admin && npm install",
    "tsc": "tsc",
    "build": "npx webpack --config ./webpack/webpack.config.prod.js",
    "start": "npx webpack server --env mode=development --config ./webpack/webpack.config.dev.js",
    "start-sub-vue": "cd ./sub-projects/vue-admin && npm start",
    "start-single-vue": "cd ./sub-projects/vue-admin && npm run start-single",
    "start-sub-react": "cd ./sub-projects/react-admin && npm start",
    "start-single-react": "cd ./sub-projects/react-admin && npm run start-single",
    "start-mock": "cd ./mock && npm start",
    "start-all": "npx concurrently \"npm run start-mock\" \"npm run start-sub-vue\" \"npm run start-sub-react\" \"npm run start\"",
    "deploy": "npm run build && cd dist && npx http-server -p 8010 --cors",
    "deploy-sub-react": "cd ./sub-projects/react-admin && npm run build && cd dist && npx http-server -p 8011 --cors",
    "deploy-sub-vue": "cd ./sub-projects/vue-admin && npm run build && cd dist && npx http-server -p 8012 --cors",
    "deploy-all": "npx concurrently \"npm run start-mock\" \"npm run deploy-sub-vue\" \"npm run deploy-sub-react\" \"npm run deploy\"",
    "prepare": "husky install"
  },
