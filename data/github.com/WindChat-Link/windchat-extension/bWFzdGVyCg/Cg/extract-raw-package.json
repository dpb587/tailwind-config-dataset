{
  "name": "windchat-extension",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev": "export $(cat .env_dev | grep -v ^# | xargs) && rm -rf dist && npm run build:hmr && (run-p wss build:watch)",
    "start": "export $(cat .env | grep -v ^# | xargs) && vite",
    "local": "export $(cat .env_local | grep -v ^# | xargs) && rm -rf dist && npm run build:hmr && (run-p wss build:watch)",
    "build": "export $(cat .env | grep -v ^# | xargs) && tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "api:swagger": "npx swagger-typescript-api -p https://chatpage-api.codexp.link/api-json -o ./src/api-swagger/generated-api --clean-output",
    "api:swagger:local": "npx swagger-typescript-api -p http://127.0.0.1:3010/api-json -o ./src/api-swagger/generated-api --clean-output",
    "wss": "node utils/reload/initReloadServer.js",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@ant-design/cssinjs": "^1.16.2",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "antd": "^5.4.7",
    "axios": "^1.4.0",
    "chokidar": "^3.5.3",
    "clsx": "^2.0.0",
    "common-tags": "^1.8.2",
    "construct-style-sheets-polyfill": "^3.1.0",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "pullstate": "^1.25.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-render-if-visible": "^2.1.1",
    "react-router-dom": "^6.12.1",
    "store": "^2.0.12",
    "tailwind-merge": "^1.14.0",
    "webextension-polyfill": "^0.10.0",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/chrome": "0.0.224",
    "@types/lodash": "^4.14.197",
    "@types/node": "18.15.11",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.11",
    "@types/store": "^2.0.2",
    "@types/webextension-polyfill": "^0.10.2",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "8.36.0",
    "fs-extra": "11.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-nested": "^6.0.1",
    "postcss-nesting": "^11.2.1",
    "rollup": "2.79.1",
    "tailwindcss": "^3.3.1",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.2",
    "tslib": "^2.6.2",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "ws": "8.13.0"
  }
}
